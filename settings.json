{
"update.mode": "none",
// for automatic update delete the entry "update.mode": "none"
"update.enableWindowsBackgroundUpdates": false,
"update.showReleaseNotes": false,
"git.ignoreMissingGitWarning": true,
"telemetry.enableTelemetry": false,
"gitlens.views.commitDetails.files.layout": "tree",
"files.autoSave": "afterDelay",
"explorer.confirmDelete": false,
"python.terminal.executeInFileDir": true,
"terminal.integrated.scrollback": 100000,
"terminal.integrated.cwd": "${workspaceFolder}",
//"workbench.editor.showTabs": "none",
"gitlab.duoChat.enabled": false,
"gitlab.pipelineGitRemoteName": "",
"git.enableSmartCommit": true,
"vsnotes.defaultNoteTitle": "{d}_{title}.{ext}",
"vsnotes.defaultSnippet": {
    "langId": "markdown",
    "name": "vsnotes"
},
"vsnotes.defaultNoteName": "notes",
"vsnotes.tokens": [
    {
        "type": "datetime",
        "token": "{dt}",
        "format": "YYYY-MM-DD_HH-mm",
        "description": "Insert formatted datetime."
    },
    {
        "type": "datetime",
        "token": "{d}",
        "format": "YYYY-MM",
        "description": "Insert formatted date."
    },
    {
        "type": "title",
        "token": "{title}",
        "description": "Insert note title from input box.",
        "format": "notes"
    },
    {
        "type": "extension",
        "token": "{ext}",
        "description": "Insert file vsnotes.",
        "format": "md"
    }
],
"git.autofetch": true,
"git.fetchOnPull": true,
"workbench.sideBar.location": "right",
"git.confirmSync": false,
"files.trimTrailingWhitespace": true,
"editor.lightbulb.enabled": "off",
"explorer.confirmDragAndDrop": false,
"workbench.iconTheme": "material-icon-theme",
"editor.fontVariations": false,
"editor.fontFamily": "'Fira Code'",
"editor.fontSize": 15,
"editor.fontWeight": "450",
"editor.fontLigatures": true,
"workbench.colorTheme": "One Monokai",
"code-runner.runInTerminal": true,
"code-runner.clearPreviousOutput": true,
"code-runner.executorMap": {
    "python": "python3"
},
"settingsSync.ignoredExtensions": [
    "gitlab.gitlab-workflow",
    "ms-vscode.powershell"
],
"workbench.startupEditor": "none",
"vsnotes.defaultNotePath": "C:\\Users\\a33351424\\OneDrive - Deutsche Telekom AG\\Hub\\90-99 Archives\\93 Logs\\93.01 VSCode Notizen",
"editor.minimap.enabled": false,
"settingsSync.ignoredSettings": [
    "vsnotes.defaultNotePath"
],
"workbench.editor.editorActionsLocation": "hidden",
"breadcrumbs.enabled": false,
"editor.stickyScroll.enabled": false,
"editor.renderWhitespace": "none",
"editor.renderControlCharacters": false,
"workbench.activityBar.location": "top",
"gitlab.duoCodeSuggestions.enabled": false,
"workbench.editorLargeFileConfirmation": 4024,
"launch": {
    "configurations": [

    ]
}
}
